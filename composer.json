{
  "name": "cspray/annotated-container",
  "description": "Create Dependency Injection containers configured with PHP8 Attributes.",
  "keywords": ["dependency injection", "container", "attributes"],
  "license": "MIT",
  "authors": [
    {
      "name": "Charles Sprayberry",
      "email": "771345+cspray@users.noreply.github.com",
      "homepage": "https://cspray.io",
      "role": "Project Maintainer"
    }
  ],
  "require": {
    "php": "^8.1",
    "cspray/typiphy": "^0.2",
    "psr/container": "^2.0",
    "nikic/php-parser": "^4.10"
  },
  "require-dev": {
    "cspray/annotated-container-dummy-apps": "0.9.1",
    "phpunit/phpunit": "^9.5",
    "mikey179/vfsstream": "^1.6"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\AnnotatedContainer\\": "src"
    },
    "files": [
      "src/Function/auto-wired-parameters.php",
      "src/Function/definitions.php",
      "src/Function/user-api.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\AnnotatedContainer\\": "test",
      "Acme\\AnnotatedContainer\\": "examples/readme-quick-start"
    }
  },
  "suggest": {
    "cspray/annotated-container-auryn": "An implementation that uses Auryn as the backing Container!",
    "cspray/annotated-container-php-di": "An implementation that uses PHP-DI as the backing Container!"
  }
}
